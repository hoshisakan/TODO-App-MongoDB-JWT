							"name": "SignIn By Normal User",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"const user = pm.response.json();\r",
											"\r",
											"pm.test(\"Has properties\", function () {\r",
											"    pm.expect(user.message).to.have.property('accessToken');\r",
											"});\r",
											"\r",
											"if (pm.test(\"Has properties\")) {\r",
											"    //console.log('Has properties.');\r",
											"    pm.globals.set('normal_user_token', user.message.accessToken);\r",
											"}\r",
											"else {\r",
											"    //console.log('Not has properties.');\r",
											"}\r",
											"\r",
											"pm.test(\"Global token has been set\", function() {\r",
											"    var token = pm.globals.get('normal_user_token');\r",
											"    pm.expect(token).to.eql(user.message.accessToken);\r",
											"    console.log(`set globals normal_user_token is: ${token}`);\r",
											"});"
										],
